<p id="notice"><%= notice %></p>

<h1>Partnerzy</h1>


<%= link_to '+ Nowy Partner', new_admin_partner_path, :class => 'float-right' %>

<table class="table">
  <thead>
    <tr>
      <th>Obraz</th>
      <th>Typ</th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @partners.each do |partner| %>
      <tr>
        <td><%= image_tag partner.image_url %></td>
        <td><%= partner.category_name %></td>
        <td><%= link_to 'Edit', edit_admin_partner_path(partner) %></td>
        <td><%= link_to 'Destroy', [:admin, partner], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
